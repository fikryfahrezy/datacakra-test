{
  "name": "back-end",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nest build",
    "format": "prettier . --write",
    "start": "nest start",
    "dev": "nest start --env-file .env --watch",
    "debug": "nest start --env-file .env --debug --watch",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:env": "node --env-file .env ./node_modules/typeorm/cli-ts-node-commonjs",
    "migrate:generate": "npm run typeorm migration:generate -- -d migration/datasource",
    "migrate:generate:env": "npm run typeorm:env migration:generate -- -d migration/datasource",
    "migrate:up": "npm run typeorm migration:run  -- -d migration/datasource",
    "migrate:up:env": "npm run typeorm:env migration:run  -- -d migration/datasource",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "@nestjs/common": "11.0.1",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.0.1",
